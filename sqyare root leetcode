class Solution {
public:
    int mySqrt(int x) {
        int a;
        int i;
         if (x == 0 || x == 1) {
        return x; // Handle base cases
    }

        for( i =1 ; i*i<=x; i++){
            a =i*i;
        if(x==a ){
             
                return i;
         }
        
         
        }
       return i-1;
    }
};
